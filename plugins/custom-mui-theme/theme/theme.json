{
  "palette": {
    "common": {
      "black": "#000",
      "white": "#fff"
    },
    "primary": {
      "main": "#2b416a",
      "light": "#fafbfb",
      "dark": "#1d1820",
      "contrastText": "#fff"
    },
    "secondary": {
